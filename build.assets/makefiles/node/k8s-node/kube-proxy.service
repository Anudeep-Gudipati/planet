[Unit]
Description=Kubernetes Proxy
Documentation=https://github.com/GoogleCloudPlatform/kubernetes

[Service]
EnvironmentFile=/etc/container-environment
ExecStart=/usr/bin/kube-proxy --master=http://${KUBE_APISERVER}:8080 --logtostderr=true --hostname-override=${KUBE_NODE_NAME}
Restart=always
RestartSec=10
