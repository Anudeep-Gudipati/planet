apiVersion: v1
kind: Config
clusters:
  - cluster:
      certificate-authority: /var/state/root.cert
      server: https://leader.telekube.local:6443
    name: kubernetes
contexts:
  - context:
      cluster: kubernetes
      user: controller-manager
    name: cm-to-kubernetes
current-context: cm-to-kubernetes
users:
  - name: controller-manager
    user:
      client-certificate: /var/state/controller-manager.cert
      client-key: /var/state/controller-manager.key
