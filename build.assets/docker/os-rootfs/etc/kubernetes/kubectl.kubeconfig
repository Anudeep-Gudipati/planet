apiVersion: v1
kind: Config
clusters:
  - cluster:
      certificate-authority: /var/state/root.cert
      server: https://leader.telekube.local:6443
    name: default
contexts:
  - context:
      cluster: default
      user: default
    name: default
current-context: scheduler-to-kubernetes
users:
  - name: default
    user:
      client-certificate: /var/state/kubectl.cert
      client-key: /var/state/kubectl.key
